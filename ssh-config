  HostName github.com
  # User git
  # ssh-keygen -o -a 100 -t ed25519 -f ~/.ssh/id_ed25519.liffft -C "emmanuel@liffft.com"
  IdentityFile ~/.ssh/id_ed25519.liffft
  ServerAliveInterval 600
  TCPKeepAlive yes
  IPQoS=throughput
  PreferredAuthentications publickey
